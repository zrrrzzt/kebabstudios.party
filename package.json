{
  "name": "kebabstudios.party",
  "description": "Frontpage for Kebab Studios",
  "version": "3.0.3",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "Geir GÃ¥sodden",
    "email": "geir.gasodden@pythonia.no",
    "url": "https://github.com/zrrrzzt"
  },
  "engines": {
    "node": ">=10.1.0"
  },
  "scripts": {
    "test": "standard && nsp check && tap --reporter=spec test/**/*.js",
    "test-offline": "standard && tap --reporter=spec test/**/*.js",
    "coverage": "tap test/**/*.js --coverage",
    "coveralls": "tap --cov --coverage-report=lcov test/**/*.js && cat coverage/lcov.info | coveralls",
    "standard-fix": "standard --fix",
    "dev": "next",
    "build": "next build && next export",
    "start": "next start",
    "now-deploy": "npm run test && npm run build && now out --static -n kebabstudios-web && now alias",
    "refresh": "rm -rf node_modules && rm package-lock.json && npm install",
    "clear": "rm -rf out && rm -rf .next"
  },
  "keywords": [
    "Kebab Studios",
    "Webpage",
    "Next.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zrrrzzt/kebabstudios.party.git"
  },
  "bugs": {
    "url": "https://github.com/zrrrzzt/kebabstudios.party/issues"
  },
  "homepage": "https://github.com/zrrrzzt/kebabstudios.party#readme",
  "now": {
    "name": "kebabstudios-web",
    "alias": [
      "kebabstudios.party"
    ],
    "env": {
      "NODE_ENV": "production"
    },
    "type": "npm",
    "engines": {
      "node": "10.1.0"
    }
  },
  "devDependencies": {
    "coveralls": "3.0.2",
    "nsp": "3.2.1",
    "standard": "12.0.1",
    "tap": "12.0.1"
  },
  "dependencies": {
    "next": "7.0.0",
    "react": "16.3.2",
    "react-dom": "16.3.2"
  }
}
